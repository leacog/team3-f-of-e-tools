#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5621e5f03b80 .scope module, "top" "top" 2 1;
 .timescale 0 0;
v0x5621e5f16fc0_0 .var "clk", 0 0;
v0x5621e5f17080_0 .net "data_out", 31 0, L_0x5621e5f17340;  1 drivers
v0x5621e5f17170_0 .var "input1", 31 0;
v0x5621e5f17270_0 .var "input2", 31 0;
S_0x5621e5f03d00 .scope module, "adder_inst" "adder" 2 8, 3 48 0, S_0x5621e5f03b80;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "input1"
    .port_info 1 /INPUT 32 "input2"
    .port_info 2 /OUTPUT 32 "out"
v0x5621e5ece080_0 .net "input1", 31 0, v0x5621e5f17170_0;  1 drivers
v0x5621e5f16da0_0 .net "input2", 31 0, v0x5621e5f17270_0;  1 drivers
v0x5621e5f16e80_0 .net "out", 31 0, L_0x5621e5f17340;  alias, 1 drivers
L_0x5621e5f17340 .arith/sum 32, v0x5621e5f17170_0, v0x5621e5f17270_0;
    .scope S_0x5621e5f03b80;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5621e5f16fc0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5621e5f03b80;
T_1 ;
    %delay 1, 0;
    %load/vec4 v0x5621e5f16fc0_0;
    %inv;
    %store/vec4 v0x5621e5f16fc0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5621e5f03b80;
T_2 ;
    %vpi_call 2 19 "$dumpfile", "adder.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5621e5f17170_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5621e5f17270_0, 0, 32;
    %delay 1, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5621e5f17170_0, 0, 32;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x5621e5f17270_0, 0, 32;
    %delay 1, 0;
    %pushi/vec4 1000, 0, 32;
    %store/vec4 v0x5621e5f17170_0, 0, 32;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x5621e5f17270_0, 0, 32;
    %delay 1, 0;
    %pushi/vec4 65535, 0, 32;
    %store/vec4 v0x5621e5f17170_0, 0, 32;
    %pushi/vec4 180158464, 0, 32;
    %store/vec4 v0x5621e5f17270_0, 0, 32;
    %delay 1, 0;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x5621e5f17170_0, 0, 32;
    %pushi/vec4 65535, 0, 32;
    %store/vec4 v0x5621e5f17270_0, 0, 32;
    %delay 1, 0;
    %pushi/vec4 65535, 0, 32;
    %store/vec4 v0x5621e5f17170_0, 0, 32;
    %pushi/vec4 65535, 0, 32;
    %store/vec4 v0x5621e5f17270_0, 0, 32;
    %delay 1, 0;
    %pushi/vec4 11468800, 0, 32;
    %store/vec4 v0x5621e5f17170_0, 0, 32;
    %pushi/vec4 184352768, 0, 32;
    %store/vec4 v0x5621e5f17270_0, 0, 32;
    %delay 1, 0;
    %vpi_call 2 56 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
